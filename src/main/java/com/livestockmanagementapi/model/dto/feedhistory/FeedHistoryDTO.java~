package com.livestockmanagementapi.model.dto.feedhistory;

import com.livestockmanagementapi.model.dto.animal.AnimalDTO;
import com.livestockmanagementapi.model.dto.employee.EmployeeDTO;
import com.livestockmanagementapi.model.dto.feedplan.FeedPlanDTO;
import com.livestockmanagementapi.model.dto.pigpen.PigPenDTO;
import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

import java.time.LocalDateTime;

@Data
@NoArgsConstructor
@AllArgsConstructor
public class FeedHistoryDTO {
    private Long id;
    private PigPenDTO pigPen;
    private AnimalDTO animal;
    private FeedPlanDTO feedPlan;
    private LocalDateTime feedingTime;
    private Long feedAmount;
    private String notes;
    private EmployeeDTO createdBy;
}
